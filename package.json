{
  "name": "plus-minus",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --extraWebpackConfig=webpack.extra.js --stats-json=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "init": "git add . && git reset --hard HEAD && git fetch origin && git pull origin",
    "build:stats": "ng build --prod --extraWebpackConfig=webpack.extra.js --stats-json && cd dist && npm run analyze",
    "build:server": "ng build -aot -app ssr",
    "analyze": "webpack-bundle-analyzer dist/plus-minus/stats.json",
    "build:test": "ng build --prod --extraWebpackConfig=webpack.extra.js --configuration=test",
    "http-server": "http-server ./dist/plus-minus -p 8080 -c-1 https://test.plusminus.az"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/material": "^8.1.2",
    "@angular/material-moment-adapter": "^8.1.2",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/platform-server": "~8.2.0",
    "@angular/pwa": "^0.802.0",
    "@angular/router": "~8.2.0",
    "@angular/service-worker": "~8.2.0",
    "@auth0/angular-jwt": "^3.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@sentry/browser": "^5.5.0",
    "bootstrap": "^4.3.1",
    "countup.js-angular2": "^7.3.0",
    "detect-browser": "^4.6.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "ng-animate": "^0.3.4",
    "ngx-awesome-uploader": "^8.0.0",
    "ngx-build-plus": "^8.1.3",
    "ngx-mat-select-search": "^1.8.0",
    "ngx-swiper-wrapper": "^8.0.1",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.0",
    "typed.js": "^2.0.10",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.0",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/node": "~12.6.8",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.0.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.5.3"
  }
